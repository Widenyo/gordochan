<center>
    <div class="iterator">
    <% if(page !== 1 && page > 6){ %>
        <a href="/boards/<%=short%>/page/<%=page%>>"><<</a>
    <% } %>
    <% for(let i = page-5; i <= page+5; i++){ %>
        <% if (i <= 0){ %>
            <% i = 1 %>
    <% } %>
        <% if(i > lastPage && i !== 1){ %>
            <% break %>
    <% } %>
        <% if(i === page){ %>
            <span><%= i %></span>
        <% }else{ %>
            <a href="/boards/<%=short%>/page/<%=i%>"><%=i%></a>
        <% } %> 

    <%} %>
    <% if(page !== lastPage && page < lastPage-5){ %>
        <a href="/boards/<%=short%>/page/<%=lastPage%>">>></a>
    <% } %>
    </div>
</center>